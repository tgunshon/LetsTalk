   "ErrorFunction": "DEMO",
   "Events": {
      "001": [
         "//Set initial values for Bar chart.",
         "$BackColor     = BarChart.AreaBackground_color",
         "$TitleText     = BarChart.Title_text",
         "$TitleFont     = BarChart.Title_style_font",
         "$TitleFontSize = conv(BarChart.Title_style_size)",
         "$ChartBarType  = BarChart.Chart_barType",
         "//",
         "//Set the chart Bar types that we can offer.",
         "//",
         "$ChartBarTypes(1) = 'bar'",
         "$ChartBarTypes(2) = 'column'",
         "$ChartBarTypes(3) = 'stacked100Bar'",
         "$ChartBarTypes(4) = 'stacked100Column'",
         "$ChartBarTypes(5) = 'stackedBar'",
         "$ChartBarTypes(6) = 'stackedColumn'",
         "//",
         "//Populate chart data file with initial values.",
         "//",
         "#seed = 137 //Use this as the seed for the data generation.",
         "lscall(\"InitialiseChartSummaryDataFile\") //in case this is not the first time this function has been",
         "                                         //executed in this session.",
         "//",
         "for #a = 1 to 12",
         "   case #a",
         "      when 1 :",
         "         Month = 'Jan'",
         "      when 2 :",
         "         Month = 'Feb'",
         "      when 3 :",
         "         Month = 'Mar'",
         "      when 4 :",
         "         Month = 'Apr'",
         "      when 5 :",
         "         Month = 'May'",
         "      when 6 :",
         "         Month = 'Jun'",
         "      when 7 :",
         "         Month = 'Jul'",
         "      when 8 :",
         "         Month = 'Aug'",
         "      when 9 :",
         "         Month = 'Sep'",
         "      when 10 :",
         "         Month = 'Oct'",
         "      when 11 :",
         "         Month = 'Nov'",
         "      when 12 :",
         "         Month = 'Dec'",
         "   endcase",
         "   //",
         "   for #b = 1 to 4",
         "      Week = 'Week ' + conv(#b)",
         "      //",
         "      for #c = 1 to 3",
         "         lscall(\"PopulateChartDetailsDataFile\")",
         "      endfor",
         "      //",
         "   endfor",
         "   //",
         "endfor",
         "//",
         "lscall(\"PopulateChartSummaryDataFile\")"
      ],
      "002": [
         "//Select all records on file if this is the initial state otherwise select specific month.",
         "",
         "if $InitialState = '' then",
         "   Month                          = ''",
         "   sel-partial(Month)",
         "   BarChart.BottomAxis_title_text = 'Months' //Set the bottom axis title text",
         "else",
         "   Month                          = $SelectMonth",
         "   sel-only(Month)",
         "   BarChart.BottomAxis_title_text = 'Weeks' //Set the bottom axis title text",
         "endif"
      ],
      "003": [
         "//Select all motnhs.",
         "Month = ''",
         "sel-partial(Month)"
      ],
      "004": [
         "//Exit to DEMO",
         "lsexit"
      ],
      "005": [
         "//Reset the line chart values to the defaults.",
         "$BackColor                    = 'White'",
         "$TitleText                    = 'Demo Bar Chart'",
         "$TitleFont                    = 'Arial Black'",
         "$TitleFontSize                = '15'",
         "$ChartBarType                 = 'column'",
         "lscall(\"PopulateChartSummaryDataFile\")",
         "BarChart.AreaBackground_color = $BackColor",
         "BarChart.Title_text           = $TitleText",
         "BarChart.Title_style_font     = $TitleFont",
         "BarChart.Title_style_size     = conv($TitleFontSize)",
         "BarChart.Chart_barType        = $ChartBarType",
         "BackgroundColor.Refresh()",
         "TitleText.Refresh()",
         "TitleFont.Refresh()",
         "TitleFontSize.Refresh()",
         "BarChartType.Refresh()",
         "$InitialState                 = ''",
         "$SelectMonth                  = ''",
         "SelMonth.Visible              = True",
         "SelWeek.Visible               = False",
         "BarChart.Refresh()",
         "Listbox.Refresh()"
      ],
      "006": [
         "//Set the chart backcolor.",
         "BarChart.AreaBackground_color = $INPUT"
      ],
      "007": [
         "//Set the chart type.",
         "BarChart.Chart_barType = $INPUT"
      ],
      "008": [
         "//Randomly generate values to make the line chart look realistic.",
         "$num = pic((#seed * #seed),'99999999')",
         "$num = $num(3,6)",
         "//",
         "if $num(len($num),len($num)) = '0'",
         "   $num = $num(1,(len($num) - 1))",
         "endif",
         "//",
         "UnitsSold(#c) = conv($num)",
         "#seed        += conv($num) + #lastnum",
         "#lastnum      = UnitsSold(#c)"
      ],
      "009": [
         "//Sort the data in month order.",
         "//",
         "case Month(1,3)",
         "   when 'Jan' :",
         "      #a = 1",
         "   when 'Feb' :",
         "      #a = 2",
         "   when 'Mar' :",
         "      #a = 3",
         "   when 'Apr' :",
         "      #a = 4",
         "   when 'May' :",
         "      #a = 5",
         "   when 'Jun' :",
         "      #a = 6",
         "   when 'Jul' :",
         "      #a = 7",
         "   when 'Aug' :",
         "      #a = 8",
         "   when 'Sep' :",
         "      #a = 9",
         "   when 'Oct' :",
         "      #a = 10",
         "   when 'Nov' :",
         "      #a = 11",
         "   when 'Dec' :",
         "      #a = 12",
         "endcase"
      ],
      "010": [
         "//Drill down",
         "$ClickedMonth = BarChart.RowId",
         "//",
         "if $ClickedMonth # '' then",
         "   //",
         "   if $InitialState = '' then",
         "      $InitialState    = 'N'",
         "      $SelectMonth     = $ClickedMonth(1,3)",
         "      SelMonth.Visible = False",
         "      SelWeek.Visible  = True",
         "   else //Clicking on a node will now return the chart to its initial state.",
         "      $InitialState    = ''",
         "      $SelectMonth     = ''",
         "      SelMonth.Visible = True",
         "      SelWeek.Visible  = False",
         "   endif",
         "   //",
         "   BarChart.Refresh()",
         "   Listbox.Refresh()",
         "endif"
      ],
      "011": [
         "//Accumulate product totals.",
         "//",
         "for #d = 1 to 3",
         "   UnitsSold_1(#d) += UnitsSold(#d)",
         "endfor"
      ],
      "012": [
         "//Set the title text.",
         "BarChart.Title_text = $INPUT"
      ],
      "013": [
         "//Set the title font.",
         "BarChart.Title_style_font = $INPUT"
      ],
      "014": [
         "//Set the title font size.",
         "BarChart.Title_style_size = conv($INPUT)"
      ],
      "017": [
         "//Set key to accumulate monthly totals.",
         "Month_1 = Month",
         "Week_1  = '*'"
      ],
      "018": [
         "//Set list key variable.",
         "$ListKey = Month(1,10) + Week",
         "//",
         "//if a specific month selected then deselect the total record.",
         "//",
         "if $InitialState # '' then",
         "   //",
         "   if Week = '*' then",
         "      dsel",
         "   endif",
         "   //",
         "else",
         "   //",
         "   if Week # '*' then",
         "      dsel",
         "   endif",
         "   //",
         "endif"
      ]
   },
